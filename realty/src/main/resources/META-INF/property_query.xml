<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="property">

<!--property mapper-->



<select id="weekinfo" resultType="property_dto">
select * from property order by pidx desc
</select>

<select id="week_tails" resultType="property_dto">
select * from property where pidx=#{pidx}
</select>


</mapper>